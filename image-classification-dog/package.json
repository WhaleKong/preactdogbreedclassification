    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "preact-cli": "^3.0.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "@tensorflow-models/mobilenet": "^2.0.4",
    "@tensorflow/tfjs": "^2.6.0",
    "bootstrap": "^4.2.1",
    "cors": "^2.8.5",
    "jquery": "^3.5.1",
    "preact": "^10.3.2",
    "preact-cli-plugin-env-vars": "^1.2.1",
    "preact-particles": "^1.18.11",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-particles-js": "^3.3.0",
    "react-tilt": "^0.1.4",
    "tachyons": "^4.12.0"
